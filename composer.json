    "psr/log": "1.0.0",
    "ramsey/uuid": "3.1.*",
    "yetanotherape/diff-match-patch": "dev-master",
    "nesbot/carbon": "1.20",
    "masterminds/html5": "^2.1",
    "thunderer/shortcode": "dev-master"
